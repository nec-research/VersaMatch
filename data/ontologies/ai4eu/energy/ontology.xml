    <!-- http://semanco02.hs-albsig.de/repository/ontology-releases/eu/semanco/ontology/SEMANCO/SEMANCO.owl#Buthane -->

    <owl:Class rdf:about="http://semanco02.hs-albsig.de/repository/ontology-releases/eu/semanco/ontology/SEMANCO/SEMANCO.owl#Buthane">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#NonRenewable"/>
        <rdfs:comment xml:lang="en">This class represents buthane energy sources. Butane is one of the hydrocarbon components of raw natural gas, which is a type of fossil fuel</rdfs:comment>
        <rdfs:label xml:lang="en">Buthane</rdfs:label>
    </owl:Class>
    


    <!-- http://semanco02.hs-albsig.de/repository/ontology-releases/eu/semanco/ontology/SEMANCO/SEMANCO.owl#LPG -->

    <owl:Class rdf:about="http://semanco02.hs-albsig.de/repository/ontology-releases/eu/semanco/ontology/SEMANCO/SEMANCO.owl#LPG">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#NonRenewable"/>
        <rdfs:comment xml:lang="en">This class represents Liquefied petroleum gas energy sources. Liquefied petroleum gas (LPG) or liquid petroleum gas (LPG or LP gas), also referred to as simply propane or butane, are flammable mixtures of hydrocarbon gases used as fuel in heating appliances, cooking equipment, and vehicles.</rdfs:comment>
        <rdfs:label xml:lang="en">LPG</rdfs:label>
    </owl:Class>
    


    <!-- http://semanco02.hs-albsig.de/repository/ontology-releases/eu/semanco/ontology/SEMANCO/SEMANCO.owl#Mix -->

    <owl:Class rdf:about="http://semanco02.hs-albsig.de/repository/ontology-releases/eu/semanco/ontology/SEMANCO/SEMANCO.owl#Mix">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#NonRenewable"/>
        <rdfs:comment xml:lang="en">This class represents mix energy sources. The energy mix is a group of different primary energy sources from which secondary energy for direct use - usually electricity - is produced. Primary energy in the form of fossil fuels is nowadays still mostly used directly for motor-driven vehicles, i.e. transport.</rdfs:comment>
        <rdfs:label xml:lang="en">Mix</rdfs:label>
    </owl:Class>
    


    <!-- http://semanco02.hs-albsig.de/repository/ontology-releases/eu/semanco/ontology/SEMANCO/SEMANCO.owl#Propane -->

    <owl:Class rdf:about="http://semanco02.hs-albsig.de/repository/ontology-releases/eu/semanco/ontology/SEMANCO/SEMANCO.owl#Propane">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#NonRenewable"/>
        <rdfs:comment xml:lang="en">This class represents propane energy sources. Propane is one of a group of liquefied petroleum gases (LP gases). The others include butane, propylene, butadiene, butylene, isobutylene, and mixtures thereof.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ww.purl.org/oema/enaeq/Oil -->

    <owl:Class rdf:about="http://ww.purl.org/oema/enaeq/Oil">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/oema/enaeq/FossilFuel"/>
        <rdfs:comment>Fraction obtained from petroleum distillation, either as a distillate or a residue.</rdfs:comment>
        <rdfs:label>Oil</rdfs:label>
    </owl:Class>
    


    <!-- http://ww.purl.org/oema/enaeq/TidalEnergySource -->

    <owl:Class rdf:about="http://ww.purl.org/oema/enaeq/TidalEnergySource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#WaterEnergySource"/>
        <rdfs:comment xml:lang="en">This class is used to model tidal power source. Tidal power, also called tidal energy, is a form of hydropower that converts the energy of tides into useful forms of power, mainly electricity.</rdfs:comment>
        <rdfs:label xml:lang="en">TidalEnergySource</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/net/v3/prosg#CoGenerationResource -->

    <owl:Class rdf:about="http://www.purl.org/net/v3/prosg#CoGenerationResource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#NonRenewable"/>
        <rdfs:comment xml:lang="en">This class models the cogeneration power producers. Cogeneration (Combined Heat and Power or CHP) is the simultaneous production of electricity and heat, both of which are useful. For instance, Gas turbines, Gas Engines, BioFuel Engines etc.</rdfs:comment>
        <rdfs:label xml:lang="en">CoGeerationResource</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/HydroResource -->

    <owl:Class rdf:about="http://www.purl.org/oema/HydroResource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Renewable"/>
        <rdfs:comment xml:lang="en">This class represents hydropower energy sources. Hydropower or water power is power derived from the energy of falling water or fast running water, which may be harnessed for useful purposes.</rdfs:comment>
        <rdfs:label xml:lang="en">HydroResource</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/NuclearEnergyResource -->

    <owl:Class rdf:about="http://www.purl.org/oema/NuclearEnergyResource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#NonRenewable"/>
        <rdfs:comment xml:lang="en">This class is used to model a nuclear-based power source. A nuclear power plant is a thermal power station in which the heat source is a nuclear reactor. As is typical in all conventional thermal power stations the heat is used to generate steam which drives a steam turbine connected to a generator which produces electricity.</rdfs:comment>
        <rdfs:label xml:lang="en">NuclearEnergySource</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/BiomassEnergyResource -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/BiomassEnergyResource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Renewable"/>
        <rdfs:comment xml:lang="en">This class represents different biomass energy sources. Biomass most often refers to plants or plant-based materials that are not used for food or feed, and are specifically called lignocellulosic biomass.[2] As an energy source, biomass can either be used directly via combustion to produce heat, or indirectly after converting it to various forms of biofuel.</rdfs:comment>
        <rdfs:label xml:lang="en">BiomassEnergySource</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/Coal -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/Coal">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/oema/enaeq/FossilFuel"/>
        <rdfs:comment>A combustible black or dark brown rock consisting chiefly of carbonized plant matter, found mainly in underground seams and used as fuel.</rdfs:comment>
        <rdfs:label>Coal</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/Electricity -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/Electricity">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#SecondaryEnergySource"/>
        <rdfs:comment>A form of energy resulting from the existence of charged particles (such as electrons or protons), either statically as an accumulation of charge or dynamically as a current.</rdfs:comment>
        <rdfs:label>Electricity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/EnergyDemand -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/EnergyDemand">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyParameter"/>
        <rdfs:comment>The amount of energy consumed by an energy consuming facility.</rdfs:comment>
        <rdfs:label xml:lang="en">Energy Demand</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/EnergySource -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/EnergySource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyType"/>
        <rdfs:label>Energy Provider/Producer View: Classification of primary and secondary energy carriers</rdfs:label>
        <rdfs:label>EnergySource</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/EnergyStorage -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/EnergyStorage">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyParameter"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.purl.org/oema/units/unitOfMeasure"/>
                <owl:someValuesFrom rdf:resource="http://www.purl.org/oema/units/EnergyUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The energy stored by an energy storage device.</rdfs:comment>
        <rdfs:label>EnergyStorage</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/FinalEnergy -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/FinalEnergy">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Coal"/>
                    <rdf:Description rdf:about="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#ElectricEnergy"/>
                    <rdf:Description rdf:about="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Gas"/>
                    <rdf:Description rdf:about="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Oil"/>
                    <rdf:Description rdf:about="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Wood"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#EnergyType"/>
        <rdfs:comment>Energy Consumer View: energy that can be directly used in the home or be transformed into some other (useful) energy form.</rdfs:comment>
        <rdfs:label>FinalEnergy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/FossilFuel -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/FossilFuel">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#NonRenewable"/>
        <rdfs:comment>A natural fuel such as coal or gas, formed in the geological past from the remains of living organisms.</rdfs:comment>
        <rdfs:label>FossilFuel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/GeothermalEnergySource -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/GeothermalEnergySource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Renewable"/>
        <rdfs:comment xml:lang="en">This class represents different geothermal energy sources. Geothermal energy is thermal energy generated and stored in the Earth. Thermal energy is the energy that determines the temperature of matter.</rdfs:comment>
        <rdfs:label xml:lang="en">GeothermalEnergySource</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/HotDryRockThermalEnergy -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/HotDryRockThermalEnergy">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/oema/enaeq/GeothermalEnergySource"/>
        <rdfs:comment xml:lang="en">This class represents Hot dry rock thermal energy sources. Hot dry rock (HDR) is an abundant source of geothermal energy available to mankind. A vast store of thermal energy is contained in the hot—but essentially dry—impervious crystalline basement rocks found almost everywhere deep beneath the earth’s surface.</rdfs:comment>
        <rdfs:label xml:lang="en">Hot Dry Rock Thermal Energy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/HydrothermalEnergy -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/HydrothermalEnergy">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/oema/enaeq/GeothermalEnergySource"/>
        <rdfs:comment xml:lang="en">This class represents hydrothermal energy sources. Hydrothermal energy is the process of obtaining heat or energy from a large body of water. &apos;Heat&apos;, in this case should not be associated with high temperature (as it may be with geothermal energy) but rather a relative heat content or relative temperature difference.</rdfs:comment>
        <rdfs:label xml:lang="en">HydrothermalEnergy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/NaturalGas -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/NaturalGas">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/oema/enaeq/FossilFuel"/>
        <rdfs:comment>Natural gas is a fossil fuel used as a source of energy for heating, cooking, and electricity generation. It is also used as a fuel for vehicles and as a chemical feedstock in the manufacture of plastics and other commercially important organic chemicals. Fossil fuel based natural gas is a non-renewable resource.</rdfs:comment>
        <rdfs:label>NaturalGas</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/ShallowGeothermalEnergy -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/ShallowGeothermalEnergy">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/oema/enaeq/GeothermalEnergySource"/>
        <rdfs:comment xml:lang="en">A ‘ground source heat’ or ‘shallow geothermal’ system is a sustainable system of extracting renewable
heat energy from the ground, concentrating it and using it to supply heat and domestic hot water. The
same system can be used to cool a house by removing surplus heat energy and putting it into the
ground. The most efficient systems carry out both functions.</rdfs:comment>
        <rdfs:label xml:lang="en">Shallow Geothermal Energy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/SorlarEnergySource -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/SorlarEnergySource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Renewable"/>
        <rdfs:comment xml:lang="en">This class represents different solar energy sources. Solar energy is radiant light and heat from the Sun that is harnessed using a range of ever-evolving technologies such as solar heating, photovoltaics, solar thermal energy, solar architecture, molten salt power plants and artificial photosynthesis. It is an important source of renewable energy and its technologies are broadly characterized as either passive solar or active solar depending on how they capture and distribute solar energy or convert it into solar power.</rdfs:comment>
        <rdfs:label xml:lang="en">SolarEnergySource</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/enaeq/WindEnergySource -->

    <owl:Class rdf:about="http://www.purl.org/oema/enaeq/WindEnergySource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Renewable"/>
        <rdfs:comment xml:lang="en">This class is used to model Wind power resources. Wind power is the conversion of wind energy into a useful form of energy, such as using wind turbines to make electrical power</rdfs:comment>
        <rdfs:label xml:lang="en">WindEnergySources</rdfs:label>
    </owl:Class>
    


    <!-- http://www.purl.org/oema/units/EnergyUnit -->

    <owl:Class rdf:about="http://www.purl.org/oema/units/EnergyUnit"/>
    


    <!-- http://www.purl.org/oema/units/UnitOfMeasure -->

    <owl:Class rdf:about="http://www.purl.org/oema/units/UnitOfMeasure"/>
    


    <!-- http://www.purl.org/oema/units/WeightUnit -->

    <owl:Class rdf:about="http://www.purl.org/oema/units/WeightUnit"/>
    


    <!-- https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#BioGasEnergySource -->

    <owl:Class rdf:about="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#BioGasEnergySource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#Renewable"/>
        <rdfs:comment xml:lang="en">This class represents biogas energy sources. Biogas typically refers to a mixture of different gases produced by the breakdown of organic matter in the absence of oxygen. Biogas can be produced from raw materials such as agricultural waste, manure, municipal waste, plant material, sewage, green waste or food waste. Biogas is a renewable energy source.</rdfs:comment>
        <rdfs:label xml:lang="en">BioGasEnergySource</rdfs:label>
    </owl:Class>
    


    <!-- https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#ChipWoodEnergySource -->

    <owl:Class rdf:about="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#ChipWoodEnergySource">
        <rdfs:subClassOf rdf:resource="https://www.auto.tuwien.ac.at/downloads/thinkhome/ontology/EnergyResourceOntology.owl#WoodEnergySource"/>
        <rdfs:comment>Woodchips are small to medium sized pieces of wood formed by cutting or chipping larger pieces of wood such as trees, branches, logging residues, stumps, roots, and wood waste.</rdfs:comment>
