        <hto2:associatedTo rdf:resource="&hto2;OnOff"/>
    </NamedIndividual>
    


    <!-- http://vcharpenay.github.io/hto/hto.xml#energy -->

    <NamedIndividual rdf:about="&hto2;energy">
        <rdf:type rdf:resource="&hto2;HTag"/>
