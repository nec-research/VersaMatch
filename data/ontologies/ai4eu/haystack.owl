        <hto2:associatedTo rdf:resource="&hto2;PerimeterHeat"/>
    </NamedIndividual>
    


    <!-- http://vcharpenay.github.io/hto/hto.xml#point -->

    <NamedIndividual rdf:about="&hto2;point">
        <rdf:type rdf:resource="&hto2;HTag"/>
