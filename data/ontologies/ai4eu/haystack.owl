        <hto2:associatedTo rdf:resource="&hto2;LightLevel"/>
    </NamedIndividual>
    


    <!-- http://vcharpenay.github.io/hto/hto.xml#lightsGroup -->

    <NamedIndividual rdf:about="&hto2;lightsGroup">
        <rdf:type rdf:resource="&hto2;HTag"/>
